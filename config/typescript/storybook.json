{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Storybook",
  "extends": "./base.json",
  "compilerOptions": {
    "moduleResolution": "Bundler",
    "allowJs": true,
    "declaration": false,
    "declarationMap": false,
    "incremental": true,
    "jsx": "react-jsx",
    "module": "ESNext",
    "noEmit": true,
    "resolveJsonModule": true,
    "target": "ES2020"
  }
}
